START=$(date "+%s")
BIN=/sdo_analysis/bin
cd $BIN
#source activate
source configure.sh
date +"%c: Downloading from GitHub YangModels"
rundownloadgithub.sh
date +"%c: Starting runIETFstats.sh"
runIETFstats.sh
date +"%c: Starting runYANGGenericstats.sh"
runYANGgenericstats.sh
date +"%c: Starting rename-file-backup.py"
mkdir -p $BACKUPDIR
python rename-file-backup.py
date +"%c: Starting runGenerateMainPage.sh"
runGenerateMainPage.sh
date +"%c: Starting runGenerateFiguresAndStats.sh"
runGenerateFiguresAndStats.sh

# This simple files for other Yang Catalog to know which files have been created
cd $WEB_PRIVATE
ls *.json > json_links
END=$(date "+%s")
python $BIN/job_log.py --start $START --end $END --status "Success" --filename "cronjob-daily"
date +"%c: End of $0"
